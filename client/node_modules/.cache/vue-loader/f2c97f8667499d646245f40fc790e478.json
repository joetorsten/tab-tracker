bar-title>Register</v-toolbar-title>\n                <v-spacer />\n                <v-tooltip bottom>\n                  <template v-slot:activator=\"{ on }\">\n                    <v-btn :href=\"source\" icon large target=\"_blank\" v-on=\"on\">\n                      <v-icon>mdi-code-tags</v-icon>\n                    </v-btn>\n                  </template>\n                  <span>Source</span>\n                </v-tooltip>\n                <v-tooltip right>\n                  <template v-slot:activator=\"{ on }\">\n                    <v-btn\n                      icon\n                      large\n                      href=\"https://codepen.io/johnjleider/pen/pMvGQO\"\n                      target=\"_blank\"\n                      v-on=\"on\"\n                    >\n                      <v-icon>mdi-codepen</v-icon>\n                    </v-btn>\n                  </template>\n                  <span>Codepen</span>\n                </v-tooltip>\n              </v-toolbar>\n              <v-card-text>\n                <v-form>\n                  <span class=\"group pa-2\">\n                    <v-icon class=\"icon\">mdi-account</v-icon>\n                    <v-text-field\n                      label=\"email\"\n                      name=\"login\"\n                      type=\"email\"\n                      v-model=\"email\"\n                    />\n                  </span>\n                  <span class=\"group pa-2\">\n                    <v-icon class=\"icon\">mdi-lock</v-icon>\n                    <v-text-field\n                      id=\"password\"\n                      label=\"Password\"\n                      name=\"password\"\n                      type=\"password\"\n                      v-model=\"password\"\n                    />\n                  </span>\n                </v-form>\n                  <div class=\"error\" v-html=\"error\"></div>\n              </v-card-text>\n              <v-card-actions>\n                <v-spacer />\n           